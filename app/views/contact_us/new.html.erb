<div id="TabbedPanels1" class="TabbedPanels">
  <ul class="TabbedPanelsTabGroup">
    <li class="TabbedPanelsTab full-width TabbedPanelsTabSelected" tabindex="0">Contact Us</li>
  </ul>
    
  <div class="TabbedPanelsContentGroup">
    <div class="TabbedPanelsContent TabbedPanelsContentVisible" style="display: block;">
      <div class="tab_content col-xs-12">
        <%= form_for @contact_us , :class=>"col-xs-12" do |f|%>
          <ul class="col-xs-12">
            <div class="col-xs-12 col-sm-1"></div>
              
            <div class="col-xs-12 col-sm-10">
              <li>
                  <h2 class="pull-left">Subject</h2>
                  <span class="box">
                    <%= image_tag "icons/title.png" %>
                    <%= f.text_field :subject %>
                  </span>
              </li>
              
              <li>
                  <h2 class="pull-left">E-Mail</h2>
                  <span class="box">
                    <%= image_tag "icons/mail.png"%>
                    <%= f.email_field :email %>
                  </span>
              </li>
              
              <li>
                <h2 class="pull-left">Inquiry</h2>
                <span class="box">
                  <%= image_tag "icons/description.png" ,:class=>"pull-left" %>
                  <%= f.text_area :description , :cols=>"6"%>
                </span>
              </li>
              
              <div class="col-xs-12 col-sm-4"></div>
              <div class="col-xs-12 col-sm-4">
                <li>
                  <button type="submit"><i class="fa fa-check" aria-hidden="true"></i> Submit</button>
                </li>
              </div>
              <div class="col-xs-12 col-sm-4"></div>
                
            </div>
            
            <div class="col-xs-12 col-sm-1"></div>
          </ul>
        <% end %>
      </div>
    </div>
  </div>
</div>