<h1>Orders</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Status</th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.title %></td>
        <td><%= Order::STATUS.key(order.status) %></td>
        <!-- <td><%#= review.review_time %> Second</td>
        <td><%#= review.status %></td> -->
        <!-- <td><%#= link_to 'Click To Review', admin_review_order_path(order) %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>
