<h3>Orders</h3>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Details</th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.title %></td>
        <td><%= link_to "Details" ,admin_order_details_path(order.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
